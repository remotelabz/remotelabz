<div class="modal fade {{ class | default('') }}" tabindex="-1" role="dialog" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="{{ id | default('') }}">{{ title | trans() }}</h5>
          <button type="button" class="close" data-dismiss="modal">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          {% block content %}{% endblock %}
          {% block body %}
          <div><a class="btn btn-danger" href="{{ path('delete_lab', {id: user.id}) }}">Yes</a></div>
          {% endblock %}        
        </div>
      </div>
    </div>
  </div>